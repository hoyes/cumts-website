{% extends 'CumtsMainBundle::layout.html.twig' %}

{% block pagename %}Contact Us{% endblock %}

{% block content %}

{% render 'CumtsMainBundle:Default:block' with {block_name: 'contact_us'} %}

<h2>Contact Form</h2>
<form method="post" action="{{ path('CumtsMainBundle_contact_post') }}">
    {{ form_widget(form) }}
    <p>
        <button type="submit">Send</button>
    </p>
</form>
{% endblock %}
